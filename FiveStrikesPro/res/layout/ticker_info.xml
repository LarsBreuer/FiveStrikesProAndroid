<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
>
	
<LinearLayout
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
    android:orientation="vertical"
	android:background="@drawable/hintergrund"
	android:paddingLeft="25dp"
	android:paddingRight="25dp"
>
	
	<TextView 
		android:text="@string/einstellungen"
		android:layout_width="fill_parent"
		android:layout_height="40dip"
		android:gravity="center"
		android:textColor="@color/standardfarbe"
		android:textSize="16dip"
		android:textStyle="bold"
	/>
	
	<LinearLayout
    	android:layout_width="fill_parent"
    	android:layout_height="40dip"
    	android:orientation="horizontal"
    >
	
		<CheckBox 
	    	android:id="@+id/checkbox_spieler_eingeben" 
        	android:layout_width="60dp"
        	android:layout_height="40dp"
			android:gravity="center"
			android:onClick="onCheckboxClicked"
		/>
		
		<TextView 
			android:text="@string/spieler_eingeben"
      		android:layout_width="wrap_content"
      		android:layout_height="40dip"
      		android:gravity="left|center_vertical"
      		android:textColor="@color/standardfarbe"
      		android:textSize="12dip"
    	/>
	
	</LinearLayout>

	<LinearLayout
    	android:layout_width="fill_parent"
    	android:layout_height="40dip"
    	android:orientation="horizontal"
    >

		<CheckBox 
	    	android:id="@+id/checkbox_wurfecke_eingeben" 
        	android:layout_width="60dp"
        	android:layout_height="40dp"
			android:gravity="center"
			android:onClick="onCheckboxClicked"
		/>

		<TextView 
			android:text="@string/wurfecke_eingeben"
      		android:layout_width="wrap_content"
      		android:layout_height="40dip"
      		android:gravity="left|center_vertical"
      		android:textColor="@color/standardfarbe"
      		android:textSize="12dip"
    	/>
	
	</LinearLayout>
	
	<TextView 
		android:text="@string/notiz"
		android:layout_width="fill_parent"
		android:layout_height="40dip"
		android:gravity="center"
		android:textColor="@color/standardfarbe"
		android:textSize="16dip"
		android:textStyle="bold"
	/>
	
	<EditText
	    android:id="@+id/notiz"
	    android:inputType="text"
	    android:layout_width="fill_parent"
        android:layout_height="100dp"
        android:gravity="left|top"
